<script type='text/javascript'>
$(function() {
  Dashing.widget_base_dimensions = [320, 96]
  Dashing.numColumns = 2
  Dashing.widget_margins = [0, 0]
});
</script>

<% content_for :title do %>Kart Dash<% end %>

<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="2">
      <div data-id="rpm" data-view="Number" data-suffix=" RPM" style="font-size:112px;"></div>
    </li>
    
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="mph" data-view="Number" data-suffix=" MPH" style="background-color:#96bf48;"></div>
    </li>
    
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="temp" data-view="Number" data-suffix=" CHT" style="background-color:#96bf48;"></div>
    </li>
    
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="lap" data-view="Number" data-suffix=" lap"></div>
    </li>
    
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="valuation" data-view="Number" data-suffix="rpm"></div>
    </li>
    
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="valuation" data-view="Number" data-suffix="rpm"></div>
    </li>
    
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="valuation" data-view="Number" data-suffix="rpm"></div>
    </li>
  </ul>
</div>
